+# game.libretro.desmume language file
+# Addon Name: game.libretro.desmume
+# Addon id: game.libretro.desmume
+# Addon Provider: libretro
+msgid ""
+msgstr ""
+"Project-Id-Version: game.libretro.desmume\n"
+"Report-Msgid-Bugs-To: alanwww1@xbmc.org\n"
+"POT-Creation-Date: 2017-02-04 09:56i\n"
+"PO-Revision-Date: 2017-02-04 09:56i\n"
+"Last-Translator: Kodi Translation Team\n"
+"Language-Team: Spanish (http://www.transifex.com/projects/p/xbmc-addons/language/es/)\n"
+"MIME-Version: 1.0\n"
+"Content-Type: text/plain; charset=UTF-8\n"
+"Content-Transfer-Encoding: 8bit\n"
+"Language: es\n"
+"Plural-Forms: nplurals=2; plural=(n != 1);\n"
+
+msgctxt "#30000"
+msgid "Settings"
+msgstr "Configuraciones"
+ 
+msgctxt "#30001"
+msgid "Enable Advanced Bus-Level Timing"
+msgstr "Habilitar sincronización avanzada a nivel de bus"
+
+msgctxt "#30002"
+msgid "Boot Into BIOS (interpreter and external bios only)"
+msgstr "Arranque en BIOS (solo intérprete y BIOS externo)"
+
+msgctxt "#30003"
+msgid "OpenGL: Color Depth (Restart)"
+msgstr "OpenGL: Profundidad de color (Reiniciar)"
+ 
+msgctxt "#30004"
+msgid "CPU Mode (Restart)"
+msgstr "Modo CPU (Reinicio)"
+ 
+msgctxt "#30005"
+msgid "Firmware Language"
+msgstr "Lenguaje de firmware"
+ 
+msgctxt "#30006"
+msgid "Frameskip"
+msgstr "Frameskip"
+
+msgctxt "#30007"
+msgid "Edge Marking"
+msgstr "Marcado de bordes"
+ 
+msgctxt "#30008"
+msgid "Soft3D: High-res Color Interpolation"
+msgstr "Soft3D: Interpolación de color de alta resolución"
+ 
+msgctxt "#30009"
+msgid "Soft3D: Line Hack"
+msgstr "Soft3D: Linea Hack"
+ 
+msgctxt "#30010"
+msgid "OpenGL: Multisampling AA"
+msgstr "OpenGL: Multimuestreo AA"
+
+msgctxt "#30011"
+msgid "Texture Deposterization"
+msgstr "Desposterización de texturas"
+
+msgctxt "#30012"
+msgid "Texture Scaling (xBrz)"
+msgstr "Escalado de textura (xBrz)"
+
+msgctxt "#30013"
+msgid "OpenGL: Texture Smoothing"
+msgstr "OpenGL: Suavizado de texturas"
+ 
+msgctxt "#30014"
+msgid "Soft3D: Texture Hack"
+msgstr "Soft3D: Hack de textura"
+ 
+msgctxt "#30015"
+msgid "Hybrid Layout: Cursor Always on Small Screen"
+msgstr "Diseño híbrido: Cursor siempre en pantalla pequeña"
+ 
+msgctxt "#30016"
+msgid "Hybrid Layout: Ratio"
+msgstr "Diseño híbrido: Relación"
+
+msgctxt "#30017"
+msgid "Hybrid Layout: Scale So Small Screen is 1:1px"
+msgstr "Diseño híbrido: Escala más pequeña pantalla es 1: 1px"
+
+msgctxt "#30018"
+msgid "Hybrid Layout: Show Both Screens"
+msgstr "Diseño híbrido: Mostrar ambas pantallas"
+
+msgctxt "#30019"
+msgid "Pointer Rotation"
+msgstr "Rotación del puntero"
+
+msgctxt "#30020"
+msgid "Internal Resolution"
+msgstr "Resolución interna"
+
+msgctxt "#30021"
+msgid "JIT Block Size"
+msgstr "Tamaño de bloque JIT"
+ 
+msgctxt "#30022"
+msgid "Load Game Into Memory (Restart)"
+msgstr "Cargar juego en la memoria (Reiniciar)"
+
+msgctxt "#30023"
+msgid "Microphone Button Noise Type"
+msgstr "Botón de micrófono tipo de ruido"
+ 
+msgctxt "#30024"
+msgid "Mouse Speed"
+msgstr "Velocidad del ratón"
+ 
+msgctxt "#30025"
+msgid "CPU Cores"
+msgstr "Núcleos de CPU"
+ 
+msgctxt "#30026"
+msgid "OpenGL: Depth-LEqual Polygon Facing"
+msgstr "OpenGL: Profundidad-LEqual Polígono Orientado"
+
+msgctxt "#30027"
+msgid "OpenGL Rasterizer (Restart)"
+msgstr "OpenGL Rasterizer (Reiniciar)"
+
+msgctxt "#30028"
+msgid "OpenGL: NDS Depth Calculation"
+msgstr "OpenGL: Cálculo de profundidad NDS"
+ 
+msgctxt "#30029"
+msgid "OpenGL: Shadow Polygons"
+msgstr "OpenGL: Polígonos de sombra"
+ 
+msgctxt "#30030"
+msgid "OpenGL: Special 0 Alpha"
+msgstr "OpenGL: Especial 0 Alpha"
+ 
+msgctxt "#30031"
+msgid "Pointer Colour"
+msgstr "Color del puntero"
+
+msgctxt "#30032"
+msgid "Emulated Pointer Acceleration Modifier Percent"
+msgstr "Porcentaje de modificador de aceleración de puntero emulado"
+
+msgctxt "#30033"
+msgid "Emulated Pointer Deadzone Percent"
+msgstr "Porcentaje de zona muerta de puntero emulado"
+ 
+msgctxt "#30034"
+msgid "Pointer Mode for Left Analog"
+msgstr "Modo de puntero para analógico izquierdo"
+ 
+msgctxt "#30035"
+msgid "Pointer Mode for Right Analog"
+msgstr "Modo de puntero para analógico derecho"
+
+msgctxt "#30036"
+msgid "Mouse/Pointer"
+msgstr "Puntero del ratón"
+ 
+msgctxt "#30037"
+msgid "Emulated Stylus Pressure Modifier Percent"
+msgstr "Porcentaje de modificador de presión de Stylus emulado"
+ 
+msgctxt "#30038"
+msgid "Pointer Type"
+msgstr "Tipo de puntero"
+ 
+msgctxt "#30039"
+msgid "Screen Gap"
+msgstr "Brecha de pantalla"
+
+msgctxt "#30040"
+msgid "Screen Layout"
+msgstr "Diseño de pantalla"
+
+msgctxt "#30041"
+msgid "Use External BIOS/Firmware (restart)"
+msgstr "Usar BIOS externo / Firmware (reiniciar)"
